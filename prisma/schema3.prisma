generator client3 {
  provider = "prisma-client"
  output   = "../src/generated/prisma/generate-client-db3"
}

datasource db3 {
  provider = "postgresql"
}

model Account {
  id Int @id @default(autoincrement())
  name String?  @map("fullname")
  role String @db3.VarChar(10) @default("member")
  createAt DateTime @default(now())
  email String @unique
  @@map("accounts")
}
